{
    "compilerOptions": {
      "target": "es2020",
      "module": "esnext",
      "moduleResolution": "node",
      "strict": true,
      "jsx": "react-jsx",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "skipLibCheck": true,
      "baseUrl": "./",
      "paths": {
        "@/*": ["resources/js/*"]
      },
      "types": ["vite/client"]
    }
  }
  